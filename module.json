{
  "id": "cyphersystem-activeeffects",
  "title": "Cypher System Active Effects",
  "description": "Adds support for Active Effects to Actors and Items for the Cypher System game system",
  "version": "#{VERSION}#",
  "library": "false",
  "manifestPlusVersion": "1.2.0",
  "compatibility": {
    "minimum": 11,
    "verified": 11
  },
  "authors": [
    {
			"name": "Farling",
			"url": "https://github.com/farling42",
			"email": "foundryvtt@amusingtime.uk",
			"discord": "farling",
			"ko-fi": "farling",
			"patreon": "amusingtime",
			"twitter": "@shambles42"
    }
  ],
  "relationships": {
    "systems": [
      {
        "id": "cyphersystem",
        "type": "system",
        "compatibility": [
          {
            "minimum": "2.9.0",
            "verified": "2.9.0"
          }
        ]
      }
    ],
    "requires": [{
      "id": "lib-wrapper",
      "type": "module",
      "manifest": "https://github.com/ruipin/fvtt-lib-wrapper/releases/latest/download/module.json"
  }]
},
  "esmodules": [
    "scripts/cs-active-effects.js",
    "scripts/cs-damagetrack.js",
    "scripts/cs-dragitemeffects.js",
    "scripts/cs-misc.js",
    "scripts/cs-pools.js",
    "scripts/cs-tagsrecursions.js"
  ],
  "styles": [
    "styles/cs-active-effects.css"
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "languages/en.json"
    }
  ],
  "url": "#{URL}#",
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#",
  "license": "LICENSE",
  "readme": "README.md"
}
